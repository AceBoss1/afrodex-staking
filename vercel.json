{
  "version": 2,
  "framework": "nextjs",

  "buildCommand": "npm run build",
  "installCommand": "npm install --omit=optional",
  "outputDirectory": ".next",

  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  },

  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30,
      "memory": 3008
    }
  }
}
