{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --omit=optional",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "^/staking/(.*)",
      "dest": "/staking/$1",
      "headers": {
        "X-Dashboard": "staking"
      }
    },
    {
      "src": "^/ambassador/(.*)",
      "dest": "/ambassador/$1",
      "headers": {
        "X-Dashboard": "ambassador"
      }
    },
    {
      "src": "^/community/(.*)",
      "dest": "/community/$1",
      "headers": {
        "X-Dashboard": "community"
      }
    }
  ]
}
